    <script type="text/javascript">
        (async () => {
              await loadAvatarContainer("@(RouteData.Values["userName"])");
            await callTimeLineApi("@(RouteData.Values["userName"])");
            $(".close-share-recipe").on("click", function (e) {
                $(".modal-share-post").css("display", "none");
            });
        })();

        var timelineGetMorePage = 0;
        var timelineGetMoreLimit = 10;
        function getMoreTimeLine() {
            callTimeLineApi("@(RouteData.Values["userName"])", timelineGetMoreLimit, ++timelineGetMorePage);
        };
        function openShareModal(e) {
            var recipeId = e.getAttribute("data-recipe-id");
            var recipeOwner = e.getAttribute("data-recipeOwner-name");
            callShareRecipeModalApi(recipeId, recipeOwner);
            $(".modal-share-post").css("display", "block");
        }

        window.onclick = function (event) {
            if (event.target.id == "modal-share-post") {
                $(".modal-share-post").css("display", "none");
            }
        }
    </script>
