    @RenderSection("Scripts", required: false)
    <script src="/js/site.js"></script>
    <script>
        audiojs.events.ready(function () {
            var as = audiojs.createAll();
        });
    </script>
    <script>

        $("#searchBtn").on("click", function (e) {
            e.preventDefault();
            var searchValue = $('#custom-input-search').val();
            fetch(`${BASE_API_URL}/api/recipe/read-recipename?recipeName=${searchValue}`,
                {
                    method: 'GET',
                    //headers: {
                    //      'Content-Type': 'application/json'
                    //  }
                })
                .then(res => res.json())
                .then(response => {
                    var searchValue = $('#custom-input-search').val();
                    var searchValueParam = encodeURIComponent(searchValue);
                    window.location.href = (`/RecipeSearch/${searchValueParam}`);
                    console.log(response);
                })
                .catch(error => {
                    console.error('Error:', error);
                });
        });

    </script>

</body>
