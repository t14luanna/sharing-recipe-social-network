                                            <audio id="myAudio">
                                                <source src="~/recipepress/audio/notitficationSound.mp3" type="audio/mpeg">
                                                Your browser does not support the audio element.
                                            </audio>
                                        </div>
                                        <nav class="nav-collapse header-social-icons">
                                            <ul>
                                                <li>

                                                    <a href="#" onclick="changeStatusNoti()"><i class="fa fa-bell"></i></a>
                                                    <span style="
                                                          position: absolute;
                                                          top: 5px;
                                                          right: -4px;
                                                          padding: 0px 5px;
                                                          border-radius: 50%;
                                                          background-color: red;
                                                          color: white; font-size: 12px" id="number-of-notification"></span>
                                                    <ul id="list-notification" class="scroll-notification"></ul>
                                                </li>
                                                <li><a href="/MessageChat"><i class="fa fa-envelope"></i></a></li>
                                            </ul>
                                        </nav>
                                    </div>

                                    <span class="sign-in-buttons authorized-group" style="display: none;">

                                        <a class="information" href="/account/information" id="authorized-group-username">Username</a>
                                    </span>
                                    <span class="sign-in-buttons authorized-group" id="logoutBtn">
                                        <a class="login">Đăng xuất</a>
                                    </span>
                                    <span class="sign-in-buttons unauthorized-group">
                                        <a class="login" href="/login">Đăng nhập</a>
                                        <a class="register" href="/register">Đăng ký</a>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
